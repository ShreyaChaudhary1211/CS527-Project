[<Line: +use my\space\AnotherClass;  // NOK {{Move the use declarations after the namespace declarations.}}
>, <Line: +use my\space\MyClass;       // NOK {{Add a blank line after this "use" declaration.}}
>, <Line: +namespace another\bar;      // NOK {{Add a blank line after this "namespace another\bar" declaration.}}
>, <Line: +{
>, <Line: +}
>, <Line: +namespace {}      // NOK {{Add a blank line after this "namespace" declaration.}}
>]
[<Line: -use my\space\AnotherClass;  // NOK - uses are before namespace
>, <Line: -use my\space\MyClass;       // NOK - no blank line after uses
>, <Line: -namespace another\bar;      // NOK - no blank line after namespace
>]