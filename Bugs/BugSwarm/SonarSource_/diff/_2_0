>, <Line: +package org.sonarlint.intellij.analysis;
>, <Line: +import org.junit.Before;
>, <Line: +import org.junit.Test;
>, <Line: +import static org.assertj.core.api.Assertions.assertThat;
>, <Line: +public class AnalysisThreadFactoryTest {
>, <Line: +  private AnalysisThreadFactory factory;
>, <Line: +  @Before
>, <Line: +  public void setUp() {
>, <Line: +    factory = new AnalysisThreadFactory();
>, <Line: +  }
>, <Line: +  @Test
>, <Line: +  public void check_props() {
>, <Line: +    Thread thread = factory.newThread(() -> {});
>, <Line: +    assertThat(thread.isDaemon()).isTrue();
>, <Line: +    assertThat(thread.getName()).startsWith("SonarLintAnalysis");
>, <Line: +    assertThat(thread.getPriority()).isEqualTo(Thread.MIN_PRIORITY);
>, <Line: +  }
