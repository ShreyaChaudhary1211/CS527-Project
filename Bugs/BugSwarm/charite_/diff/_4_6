>, <Line: +		useThreeLetterAminoAcidCode = args.getBoolean("3_letter_amino_acids");
>, <Line: +		nt3PrimeShifting = !args.getBoolean("3_prime_shifting");
>, <Line: +		databaseFilePath = args.getString("database");
>, <Line: +		showAll = args.getBoolean("show_all");
>, <Line: +	}
>, <Line: +	public boolean isUseThreeLetterAminoAcidCode() {
