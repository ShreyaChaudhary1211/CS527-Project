[<Line: +package org.kairosdb.rollup;
>, <Line: +import com.google.inject.AbstractModule;
>, <Line: +import com.google.inject.Scopes;
>, <Line: +import com.google.inject.name.Names;
>, <Line: +import org.kairosdb.core.http.rest.RollUpResource;
>, <Line: +public class RollUpModule extends AbstractModule
>, <Line: +{
>, <Line: +	@Override
>, <Line: +	protected void configure()
>, <Line: +	{
>, <Line: +		bind(RollUpResource.class).in(Scopes.SINGLETON);
>, <Line: +		bind(RollUpManager.class).in(Scopes.SINGLETON);
>, <Line: +		bind(RollUpTasksStore.class).to(RollUpTasksFileStore.class).in(Scopes.SINGLETON);
>, <Line: +		bind(RollUpJob.class);
>, <Line: +		bindConstant().annotatedWith(Names.named("STORE_DIRECTORY")).to("/tmp");
>, <Line: +	}
>, <Line: +}
>]
