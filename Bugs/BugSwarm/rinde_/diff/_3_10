>, <Line: +package com.github.rinde.rinsim.experiment.base;
>, <Line: +import static com.google.common.base.MoreObjects.toStringHelper;
>, <Line: +import static java.util.Objects.requireNonNull;
>, <Line: +import javax.annotation.Nullable;
>, <Line: +import com.google.common.collect.ComparisonChain;
>, <Line: +public class DefaultScenario implements Scenario {
>, <Line: +  protected final ProblemClass problemClass;
>, <Line: +  protected final String instanceId;
>, <Line: +  public DefaultScenario(ProblemClass pc, String id) {
>, <Line: +    problemClass = pc;
>, <Line: +    instanceId = id;
>, <Line: +  }
>, <Line: +  @Override
>, <Line: +  public ProblemClass getProblemClass() {
>, <Line: +    return problemClass;
>, <Line: +  }
>, <Line: +  @Override
>, <Line: +  public String getProblemInstanceId() {
>, <Line: +    return instanceId;
>, <Line: +  }
>, <Line: +  @Override
>, <Line: +  public int compareTo(@Nullable Scenario o) {
>, <Line: +    requireNonNull(o);
>, <Line: +    return ComparisonChain.start()
>, <Line: +        .compare(problemClass, o.getProblemClass())
>, <Line: +        .compare(instanceId, o.getProblemInstanceId())
>, <Line: +        .result();
>, <Line: +  }
>, <Line: +  @Override
>, <Line: +  public String toString() {
>, <Line: +    return toStringHelper("DefaultScenario")
>, <Line: +        .add("problemClass", problemClass)
>, <Line: +        .add("instanceId", instanceId)
>, <Line: +        .toString();
>, <Line: +  }
>, <Line: +}
>]
