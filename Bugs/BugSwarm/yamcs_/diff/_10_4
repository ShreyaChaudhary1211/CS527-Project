[<Line: +package org.yamcs.algorithms;
>, <Line: +import java.util.ArrayList;
>, <Line: +import java.util.List;
>, <Line: +import java.util.Set;
>, <Line: +import org.yamcs.parameter.ParameterValue;
>, <Line: +import org.yamcs.xtce.Algorithm;
>, <Line: +import org.yamcs.xtce.Parameter;
>, <Line: +/**
>, <Line: + * Represents the execution context of one algorithm. An AlgorithmExecutor is reused
>, <Line: + * upon each update of one or more of its InputParameters.
>, <Line: + *  
>, <Line: + */
>, <Line: +public interface AlgorithmExecutor {
>, <Line: +    Algorithm getAlgorithm();
>, <Line: +    Set<Parameter> getRequiredParameters();
>, <Line: +    int getLookbackSize(Parameter parameter);
>, <Line: +    /**
>, <Line: +     * Update parameters and return true if the algorithm should run
>, <Line: +     * 
>, <Line: +     * @param paramList - list of input parameters
>, <Line: +     * @return true if the algorithm should run
>, <Line: +     */
>, <Line: +    boolean updateParameters(ArrayList<ParameterValue> paramList);
>, <Line: +    /**
>, <Line: +     * Runs the associated algorithm with the latest InputParameters
>, <Line: +     * @param acqTime 
>, <Line: +     * @param genTime 
>, <Line: +     * @return the outputted parameters, if any
>, <Line: +     */
>, <Line: +    List<ParameterValue> runAlgorithm(long acqTime, long genTime);
>, <Line: +    void addExecListener(AlgorithmExecListener listener);
>, <Line: +    AlgorithmExecutionContext getExecutionContext();
>, <Line: +}
