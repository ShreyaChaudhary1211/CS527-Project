>, <Line: +		mv.visitLineNumber(33, l0);
>, <Line: +		mv.visitVarInsn(ALOAD, 0);
>, <Line: +		mv.visitVarInsn(ALOAD, 1);
>, <Line: +		mv.visitMethodInsn(INVOKESPECIAL, "com/artemis/EntitySystem", "setWorld", "(Lcom/artemis/World;)V", false);
>, <Line: +		Label l1 = new Label();
>, <Line: +		mv.visitLabel(l1);
>, <Line: +		mv.visitLineNumber(34, l1);
>, <Line: +		mv.visitVarInsn(ALOAD, 0);
>, <Line: +		mv.visitVarInsn(ALOAD, 0);
>, <Line: +		mv.visitMethodInsn(INVOKEVIRTUAL, owner, "createFlyweightEntity", "()Lcom/artemis/Entity;", false);
>, <Line: +		mv.visitFieldInsn(PUTFIELD, owner, "flyweight", "Lcom/artemis/Entity;");
>, <Line: +		Label l2 = new Label();
>, <Line: +		mv.visitLabel(l2);
>, <Line: +		mv.visitLineNumber(35, l2);
>, <Line: +		mv.visitInsn(RETURN);
>, <Line: +		Label l3 = new Label();
>, <Line: +		mv.visitLabel(l3);
>, <Line: +		mv.visitLocalVariable("this", meta.type.toString(), null, l0, l3, 0);
>, <Line: +		mv.visitLocalVariable("world", "Lcom/artemis/World;", null, l0, l3, 1);
>, <Line: +		mv.visitEnd();
>, <Line: +	}
