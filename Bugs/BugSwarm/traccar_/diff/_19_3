>, <Line: +package org.traccar.protocol;
>, <Line: +import org.jboss.netty.bootstrap.ConnectionlessBootstrap;
>, <Line: +import org.jboss.netty.channel.ChannelPipeline;
>, <Line: +import org.traccar.BaseProtocol;
>, <Line: +import org.traccar.TrackerServer;
>, <Line: +import java.util.List;
>, <Line: +public class SmokeyProtocol extends BaseProtocol {
>, <Line: +    public SmokeyProtocol() {
>, <Line: +        super("smokey");
>, <Line: +    }
>, <Line: +    @Override
>, <Line: +    public void initTrackerServers(List<TrackerServer> serverList) {
>, <Line: +        serverList.add(new TrackerServer(new ConnectionlessBootstrap(), getName()) {
>, <Line: +            @Override
>, <Line: +            protected void addSpecificHandlers(ChannelPipeline pipeline) {
>, <Line: +                pipeline.addLast("objectDecoder", new SmokeyProtocolDecoder(SmokeyProtocol.this));
>, <Line: +            }
>, <Line: +        });
>, <Line: +    }
>, <Line: +}
>]
[]