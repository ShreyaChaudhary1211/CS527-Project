>, <Line: +package org.traccar.api.resource;
>, <Line: +import org.traccar.Context;
>, <Line: +import org.traccar.api.BaseResource;
>, <Line: +import org.traccar.model.Statistics;
>, <Line: +import org.traccar.web.JsonConverter;
>, <Line: +import javax.ws.rs.Consumes;
>, <Line: +import javax.ws.rs.GET;
>, <Line: +import javax.ws.rs.Path;
>, <Line: +import javax.ws.rs.Produces;
>, <Line: +import javax.ws.rs.QueryParam;
>, <Line: +import javax.ws.rs.core.MediaType;
>, <Line: +import java.sql.SQLException;
>, <Line: +import java.util.Collection;
>, <Line: +@Path("statistics")
>, <Line: +@Produces(MediaType.APPLICATION_JSON)
>, <Line: +@Consumes(MediaType.APPLICATION_JSON)
>, <Line: +public class StatisticsResource extends BaseResource {
>, <Line: +    @GET
>, <Line: +    public Collection<Statistics> get(
>, <Line: +            @QueryParam("from") String from, @QueryParam("to") String to) throws SQLException {
>, <Line: +        Context.getPermissionsManager().checkAdmin(getUserId());
>, <Line: +        return Context.getDataManager().getStatistics(JsonConverter.parseDate(from), JsonConverter.parseDate(to));
>, <Line: +    }
>, <Line: +}
>]
