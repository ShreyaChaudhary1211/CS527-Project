[<Line: +	<select id="getPersonItemPairs" resultMap="personItemPairResult">
>, <Line: +		select p.id as person_id, p.name as person_name, i.id as item_id, i.name as item_name
>, <Line: +		from persons p, items i
>, <Line: +		where p.id = i.owner
>, <Line: +		order by p.id, i.id
>, <Line: +	</select>
